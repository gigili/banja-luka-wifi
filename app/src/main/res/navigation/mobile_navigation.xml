<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.gac.banjalukawifi.ui.HomeFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_navigation_home_to_navigation_add_edit"
            app:destination="@id/navigation_add_edit" />
    </fragment>

    <fragment
        android:id="@+id/navigation_add_edit"
        android:name="com.gac.banjalukawifi.ui.AddEditNetworkFragment"
        android:label="@string/title_add_edit_network"
        tools:layout="@layout/fragment_add_edit_network" >
        <action
            android:id="@+id/action_navigation_add_edit_to_navigation_home"
            app:destination="@id/navigation_home" />
    </fragment>

</navigation>